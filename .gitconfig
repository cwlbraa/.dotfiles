[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
	sur = submodule update --init --recursive --jobs=7
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ctags = !$(git rev-parse --git-dir)/hooks/ctags
